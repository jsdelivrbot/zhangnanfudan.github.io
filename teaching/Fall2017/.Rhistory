sumdice <- function(n)
sum(sample(1:6, size=n, replace=TRUE))
sumdice <- function(n, sides = 6) {
if (sides < 1) return (0)
k <- sample(1:sides, size=n, replace=TRUE)
return(sum(k))
}
sumdice(5) # default 6 sides
sumdice(n=5, sides=4) # 4 sides
